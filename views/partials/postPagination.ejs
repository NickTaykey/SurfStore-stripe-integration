<% if(posts.page-1){ %>
    <a href="<%= paginateUrl %><%= posts.page-1 %>">Prev</a>
<% } %> 
<% if(posts.pages>1){ %>
    <% for(let i = 1; i<=posts.pages; i++){ %>
        <a href="<%= paginateUrl %><%= i %>" <%= posts.page===i ? "style=color:#000;" : '' %> ><%= i %></a>
    <% } %>
<% } %>
<% if(posts.page!==posts.pages){ %>
    <a href="<%= paginateUrl %><%= posts.page+1 %>">Next</a>
<% } %>
    